		<array name="modifiers">
			<dict>
				<string name="class">Modifiers::ConvertStat</string>
				<string name="from">HealthRegen</string>
				<string name="to">HealthRegen</string>
				<float name="mul">%%AMOUNT%%</float>
				<bool name="add">false</bool>
			</dict>
			<dict>
				<string name="class">Modifiers::ConvertStat</string>
				<string name="from">ManaRegen</string>
				<string name="to">ManaRegen</string>
				<float name="mul">%%AMOUNT%%</float>
				<bool name="add">false</bool>
			</dict>
		</array>
	</dict>
%endblock

