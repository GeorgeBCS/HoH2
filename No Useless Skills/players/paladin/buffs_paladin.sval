%defblock Shackles LVL DMG SPEED
	<dict name="judgement_shackles_%%LVL%%">
		<array name="tags">
			<string>slow</string>
		</array>

		<bool name="debuff">true</bool>
		<int name="duration">3000</int>

		<!-- <float name="dmg-taken-mul">%%DMG%%</float> -->
		<float name="speed-mul">%%SPEED%%</float>
		<string name="icon">special/icons/slow.unit</string>

		<dict name="tick">
			<int name="freq">500</int>
			<bool name="immediate">true</bool>

			<array name="effects">
				<dict>
					<float name="enemy-dmg">1</float>
				</dict>
				<dict>
					<string name="class">Damage</string>
					<int name="physical">%%DMG%%</int>
					<bool name="dot">true</bool>
					<bool name="true-strike">true</bool>
				</dict>
			</array>
		</dict>

		<string name="category">shackles</string>
		<int name="category-level">%%LVL%%</int>
	</dict>
%endblock

%block Shackles 1 10 0.4
%block Shackles 2 20 0.3
%block Shackles 3 30 0.2






